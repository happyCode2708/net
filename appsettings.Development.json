{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={0},{1};Database={2};User Id={3};Password={4};TrustServerCertificate=True;"
  },
  "AllowedHosts": "*",
  "StorageConfig": {
    "MaxFileSize": 10485760,
    "DefaultAssetPath": "assets",
    "AssetPathRequest": "/assets"
  }
}
